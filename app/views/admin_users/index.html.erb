<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
    <title>Feed Viewer 3</title>

    <!--script language="javascript" src="../state/save-state.php"></script -->
    <!-- script language="javascript" src="../state/get-state.php"></script -->
    <!-- script language="javascript" src="../state/SessionProvider.js"></script -->
    <!-- link rel="stylesheet" type="text/css" href="../../resources/css/ext-all.css" / -->

    <%= stylesheet_link_tag "ext-all" %>
    <%= stylesheet_link_tag "feed-viewer" %>

    <%= javascript_include_tag "ext/adapter/ext/ext-base" %>
    <%= javascript_include_tag "ext/ext-all" %>

    <%= javascript_include_tag "feed-viewer/TabCloseMenu" %>
    <%= javascript_include_tag "tpd/UserSearchField" %>
    <%= javascript_include_tag "tpd/admin-config/FeedViewer" %>
    <%= javascript_include_tag "tpd/admin-config/FeedGrid" %>
    <%= javascript_include_tag "tpd/admin-config/MainPanel" %>
    <%= javascript_include_tag "tpd/admin-config/FeedPanel" %>
    <%= javascript_include_tag "tpd/admin-config/ShippingPanel" %>
    <%= javascript_include_tag "tpd/admin-config/WarrantyGrid" %>
    <%= javascript_include_tag "tpd/admin-config/BrandGrid" %>

</head>
<body>
<div id="header"><div style="float:right;margin:5px;" class="x-small-editor"></div></div>
<div id="entry-div"></div>
<!-- Template used for Feed Items -->
<textarea id="preview-tpl" style="display:none;">
    <div class="post-data">
        <span class="post-date">{pubDate:date("M j, Y, g:i a")}</span>
        <h3 class="post-title">{title}</h3>
        <h4 class="post-author">by {author:defaultValue("Unknown")}</h4>
    </div>
    <div class="post-body">{content:this.getBody}</div>
</textarea>

</body>
</html>
